{
  "name": "ilifeandcolour",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "super": "supervisor ./bin/www",
    "migrate": "sequelize db:migrate --url sqlite://$(pwd)/instant.sqlite",
    "seed": "sequelize db:seed:all --url sqlite://$(pwd)/instant.sqlite",
    "migrate_win": "sequelize db:migrate --url sqlite://%cd%/instant.sqlite",
    "seed_win": "sequelize db:seed:all --url sqlite://%cd%/instant.sqlite",
    "migrate_env": "sequelize db:migrate --url $DATABASE_URL",
    "seed_env": "sequelize db:seed:all --url $DATABASE_URL"
  },
  "dependencies": {
    "connect-session-sequelize": "^7.1.1",
    "cookie-parser": "~1.4.4",
    "cookieconsent": "^3.1.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-flash": "0.0.2",
    "express-http-to-https": "^1.1.4",
    "express-partials": "^0.3.0",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "js2xmlparser": "^4.0.1",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.1.1",
    "supervisor": "^0.12.0"
  }
